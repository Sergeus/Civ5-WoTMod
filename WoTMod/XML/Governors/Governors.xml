			<Strategy>TXT_KEY_GOVERNOR_BLACKSMITH_STRATEGY</Strategy>
			<Help>TXT_KEY_GOVERNOR_BLACKSMITH_HELP</Help>
		</Row>
	</Governors>
	<Governor_YieldChanges>
